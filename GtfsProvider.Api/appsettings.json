{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Loki",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentName"
    ],
    "Properties": {
      "Application": "GtfsProvider"
    },
    "WriteTo": {
      "ConsoleSink": {
        "Name": "Console"
      },
      "LokiSink": {
        "Name": "LokiHttp",
        "Args": {
          "serverUrl": "",
          "username": "",
          "password": "",
          "labelProvider": "Namespace.ClassName, Assembly"
        }
      }
    }
  },
  "AllowedHosts": "*"
}
